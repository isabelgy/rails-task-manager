<% @tasks.each do |t| %>
<p><%= t.completed %>
  <%= link_to t.title, task_path(t)%>
  <%= link_to "Delete", task_path(t),
      method: :delete,
      data: { confirm: "Are you sure?" } %>
  <%= link_to "update a new task", edit_path(t) %>
</p>
<% end %>

<%= link_to "add a new task", new_path %>

